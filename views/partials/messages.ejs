

<% if(typeof errors != 'undefined'){ %> 
<% errors.forEach(function(error) { %>
<div class="alert fa fa-times-circle">
  <%= error.msg %>
</div>
<% }); %> 
<% } %> 

<% if(success_msg != ''){ %>
<div class="alert fa fa-times-circle">
  <%= success_msg %>
  </div>
<% } %>


 <% if(error_msg != ''){ %>
 <div class="alert fa fa-times-circle">
  <%= error_msg %>
  </div>
<% } %> 

<% if(error != ''){ %>
<div class="alert fa fa-times-circle">
  <%= error %>
</div>
<% } %>
